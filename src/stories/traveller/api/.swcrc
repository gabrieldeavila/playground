{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "baseUrl": "./",
    "paths": {
      "@src/*": ["src/*"],
      "@core/*": ["src/core/*"],
      "@contexts/*": ["src/contexts/*"],
      "@shared/*": ["src/shared/*"],
      "@tests/*": ["tests/*"]
    }
  },
  "minify": true
}
